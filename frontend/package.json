{
  "name": "llm-logic-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run dev:mock | pnpm run dev:local --port 3000",
    "dev:local": "next dev",
    "dev:mock": "cd ../backend && poetry install --no-root && poetry run python app.py",
    "build": "next build",
    "start": "pnpm run dev:mock | next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "knip:fix": "knip --fix",
    "check:knip": "knip --reporter markdown",
    "check:format": "prettier --check .",
    "check:type": "tsc --noEmit",
    "check:all": "pnpm run lint && pnpm run check:format && pnpm run check:type && pnpm run check:knip",
    "test": "jest",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.16.0",
    "@mui/system": "^5.16.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-oauth/google": "^0.12.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "knip": "^5.26.0",
    "lucide-react": "^0.400.0",
    "next": "14.2.4",
    "nookies": "^2.5.2",
    "react": "^18.2",
    "react-dom": "^18.2",
    "react-icons": "^5.2.1",
    "sass": "^1.77.8",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-unused-imports": "^3.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3"
  }
}
